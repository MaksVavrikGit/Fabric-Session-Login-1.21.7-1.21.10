{
	"schemaVersion": 1,
	"id": "session-id-login-mod",
	"version": "${version}",
	"name": "Session ID Login Mod",
	"description": "A lightweight Fabric client-side mod that adds a GUI to log in using Minecraft session IDs. Includes a status display (Valid/Invalid) and integrates cleanly into the multiplayer menu without affecting gameplay or server-side behavior.",
	"authors": [
		"MaksVavrik","Majanito"
	],
	"contact": {
		"homepage": "None",
		"sources": "https://github.com/Majanito/Fabric-Session-Login"
	},
	"license": "CC0-1.0",
	"icon": "assets/session-id-login-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.maksvavrik.SessionIDLoginMod"
		]
	},
	"mixins": [
		{
			"config": "loginmod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.5",
		"minecraft": "*",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}